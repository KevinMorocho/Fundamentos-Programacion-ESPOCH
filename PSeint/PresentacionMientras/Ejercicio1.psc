Proceso Ejercicio1
	//Ver si dos numeros son iguales
	Definir numero1,numero2,i Como Entero;
	i<-0;
	Mientras i==0 Hacer
		Escribir "Ingrese el primer numero: "; leer numero1;
		Escribir "Ingrese el segundo numero: "; leer numero2;
		Si numero1==numero2 Entonces
			i<-1;
			Escribir "Los numeros son iguales";
		SiNo
			Escribir "Los numeros no son iguales, intente de nuevo";
		FinSi
	FinMientras

FinProceso
